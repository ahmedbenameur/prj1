pipeline {
    agent any
    tools {
        jdk 'jdk-17' // Make sure the JDK version name matches the one in Jenkins' Global Tool Configuration
    }
    stages {
        stage('Build') {
            steps {
                sh './mvnw clean verify sonar:sonar -Dsonar.host.url=http://so-sonarqube-1:9000'
            }
        }
    }
}

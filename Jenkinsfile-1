pipeline {
 agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') { 
           sh './mvnw clean verify sonar:sonar -Dsonar.host.url=http://so-sonarqube-1:9000'
        }
      }
    }
  }
}
